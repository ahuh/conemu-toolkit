{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "mapped_shell_names": {
              "bash": "\ue691",
              "cmd": "\uf120 ",
              "pwsh": "\uebc7 ",
              "sh": "\ue691"
            }
          },
          "template": "{{ .Name }}",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "#FFFFFF",
          "type": "shell",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ued25 ",
          "foreground": "#FFFFFF",
          "powerline_symbol": "\ue0b0",
          "background": "#DA3700",
          "type": "root",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\uf2be {{ .UserName }}@{{ .HostName }} ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "#0CC616",
          "type": "session",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "\uf141",
            "folder_separator_icon": " \ue0b1 ",
            "home_icon": "\ueb06",
            "max_depth": 3,
            "style": "agnoster_short"
          },
          "template": "{{ path \"\uea83\" .Location }} \ue0b1 {{ .Path }} ",
          "foreground": "#FFFFFF",
          "powerline_symbol": "\ue0b0",
          "background": "#5648E7",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "environment",
            "fetch_virtual_env": true,
            "home_enabled": true
          },
          "template": "\ue235 {{ if .Venv }}{{ .Venv }} {{ end }}",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "#C78836",
          "type": "python",
          "style": "powerline"
        },
        {
          "properties": {
            "parse_kubeconfig": true
          },
          "template": "\uf333 {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}} ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "#fff01e",
          "type": "kubectl",
          "style": "powerline"
        },
        {
          "properties": {
            "branch_icon": "\ue725 ",
            "cache_duration": "none",
            "cherry_pick_icon": "âœ“ ",
            "commit_icon": "\u25b7 ",
            "fetch_status": true,
            "merge_icon": "\u25f4 ",
            "no_commits_icon": "\uf594 ",
            "rebase_icon": "\u2c62 ",
            "tag_icon": "\u25b6 "
          },
          "template": "{{ .HEAD }}{{ if .Working.Changed}} \uf044{{end}}{{ if .Staging.Changed}} \uf046{{end}}{{ if gt .StashCount 0 }} \uf0c7{{ end }}{{ if or (gt .Ahead 0) (gt .Behind 0) }} \ue0b1 {{.Behind}}\u2193 {{.Ahead}}\u2191{{end}} ",
          "foreground": "#FFFFFF",
          "powerline_symbol": "\ue0b0",
          "background": "#981788",
          "type": "git",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\uf071 ",
          "foreground": "#FFFFFF",
          "powerline_symbol": "\ue0b0",
          "background": "#DA3700",
          "type": "status",
          "style": "powerline"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#767676>\u2570\u2500\uf054</> {{ if .WSL }}{{ .PWD }}{{else}}{{ .Location }}{{end}} <#767676>\ue691</>",
          "foreground": "#0CC616",
          "type": "path",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
