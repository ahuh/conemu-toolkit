{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#888888",
          "foreground": "#FFFFFF",
          "leading_powerline_symbol": "\ue0b2",
          "properties": {
            "mapped_shell_names": {
              "cmd": "\uf120",
              "pwsh": "\uebc7",
              "bash": "\ue691",
              "sh": "\ue691"
            }
          },
          "style": "powerline",
          "template": "{{ .Name }} ",
          "powerline_symbol": "\ue0b0",
          "type": "shell"
        },
        {
          "background": "#AA0000",
          "foreground": "#FFFF55",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ued25 ",
          "type": "root"
        },
        {
          "background": "#00AA00",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " {{ .UserName }}@{{ .HostName }} ",
          "type": "session"
        },
        {
          "background": "#0000AA",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "folder_icon": "\ueaf7",
            "folder_separator_icon": " \ue0b1 ",
            "home_icon": "\ueb06",
            "style": "agnoster_short",
            "max_depth": 3
          },
          "style": "powerline",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#AA00AA",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "cherry_pick_icon": "\u2713 ",
            "commit_icon": "\u25b7 ",
            "fetch_status": true,
            "merge_icon": "\u25f4 ",
            "no_commits_icon": "[no commits]",
            "rebase_icon": "\u2c62 ",
            "tag_icon": "\u25b6 "
          },
          "style": "powerline",
          "template": "{{ .HEAD }}{{ if and (eq .Ahead 0) (eq .Behind 0) }} \u2261{{end}}{{ if gt .Ahead 0 }} \u2191{{.Ahead}}{{end}}{{ if gt .Behind 0 }} \u2193{{.Behind}}{{end}} {{ if .Working.Changed }}+{{ .Working.Added }} ~{{ .Working.Modified }} -{{ .Working.Deleted }} {{ end }}",
          "type": "git"
        },
        {
          "background": "#FF5555",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \uf071 ",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}